{
    "plugins":["react","react-native","import"],
    "rules":{
        "prettier/prettier":"off",
        "quotes":[2,"double"],
        "indent":[2,4],
        "comma-dangle":"off",
        "prefer-const":"off",
        "no-multi-spaces":[2,{
            "ignoreEOLComments":false,
            "exceptions":{
                "Property":false,
                "VariableDeclarator":false,
                "BinaryExpression":false,
                "ImportDeclaration":false
            }
        }],
        "key-spacing":[2,{
            "mode":"strict",
            "beforeColon":false,
            "afterColon":false
        }],
        "no-trailing-spaces":[2,{
            "skipBlankLines":true,
            "ignoreComments":true
        }],
        "no-empty-pattern":"off",
        "no-unused-vars":"off",
        "object-curly-spacing":[2,"never"],
        "object-curly-newline":[2,{
            "ImportDeclaration":{"consistent":true},
            "ExportDeclaration":{"multiline":true,"minProperties":3}
        }],
        "function-call-argument-newline":[2,"never"],
        "array-bracket-spacing":[2,"never"],
        "brace-style":[2,"stroustrup",{"allowSingleLine":true}],
        "space-before-function-paren":[2,"never"],
        "space-before-blocks":[2,"never"],
        "space-infix-ops":[0,"never"],
        "space-unary-ops":[2,{
            "words":false,
            "nonwords":false
        }],
        "semi":"off",
        "computed-property-spacing":[2,"never"],
        "semi-spacing":["error",{"before":false,"after":false}],
        "keyword-spacing":[2,{
            "before":false,
            "after":false,
            "overrides":{
                "import":{"before":true,"after":true},
                "export":{"before":true,"after":true},
                "from":{"before":true,"after":true},
                "return":{"before":true,"after":true}
            }
        }],
        "arrow-spacing":[2,{"before":false,"after":false}],
        "react/self-closing-comp":["error",{
            "component":false,
            "html":false
        }],
        "react-native/no-color-literals":[0,"never"],
        "react-native/sort-styles":[0,"never"],
        "react-native/no-single-element-style-arrays":[0,"never"],
        "react-native/no-inline-styles":[0,"never"],
        "react/jsx-tag-spacing":[2,{
            "beforeSelfClosing":"never"
        }]
    },
    "ignorePatterns":["jest/*"],
    "parserOptions":{
        "ecmaVersion":"latest",
        "ecmaFeatures":{
            "jsx":true
        },
        "sourceType":"module",
        "warnOnUnsupportedTypeScriptVersion":false
    }
}